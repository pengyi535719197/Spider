        <script>
			var IPAlert= ('<form onsubmit="goValidate();return false;">')
						  +('<p>您的计算机或网络可能正在发送自动查询，为了保护我们的用户，我们目前无法处理您的请求您的刷新超速，若解除IP屏蔽，请输入检证码。</p>')
						  +('<p>')
						  +('<span>要继续搜索，请输入验证码：</span><input type="text" id="IPValidate"  class="txt"/>')
						  +('<img id="ipValidateImg" src="" alt="正在下载验证码"/>')
						  +('<a href="#" onclick="IPValidateChangeImg();return false;">更换验证码</a>')
						  +('</p>')
						  +('<div><input type="submit" name="IPValidateSubmit" id="IPValidateSubmit" value="提交" /></div>')	
						  +('</form>');						  
				function ready(){
				//var ipValidateDiv = document.createElement("div");					
					 // ipValidateDiv.innerHTML = IPAlert;
					 // document.getElementById("code").appendChild(ipValidateDiv);
					  document.getElementById("code").innerHTML = IPAlert;
					  document.getElementById('ipValidateImg').style.display = 'none';
					  document.getElementById('ipValidateImg').src = '';
					  document.getElementById('ipValidateImg').src = "/services/code/getCode?s=0u0d_!!_5ExOO_!!_0A";
					  document.getElementById('ipValidateImg').style.display = 'block';
					  document.getElementById('ipValidateImg').style.float = '';
				 ipCheckCancel = function (){
				    //var div=document.getElementById('ipValidateDiv');
				    //div.parentNode.removeChild(div);
				    ipValidateDivSign = false;
				    ipValidateObjId = null;
					windows.close();
				  }
				  goValidate = function (){
				    var value = document.getElementById("IPValidate").value;
				    var ipValidateObj = document.createElement("script");
				    ipValidateObj.id = 'ipValidateJS'
				    ipValidateObj.language = "javascript";
				    ipValidateObj.type = "text/javascript";
				    ipValidateObj.src = "http://bus.mapbar.com:80/ip/validate.jsp?kt=0!_@3D_e!_@5Ed6d!_@2116!_@290!_@3Dx!_@5D!_@5EcOO!_@0A&code1=0u0d_!!_5ExOO_!!_0A&code2=" + value + "&" + (new Date()).getTime();
				    document.body.insertBefore(ipValidateObj, document.body.firstChild);
				  }
				  IPValidateChangeImg = function () {
				    var ipValidateObj = document.createElement("script");
				    ipValidateObj.id = 'ipValidateJS'
				    ipValidateObj.language = "javascript";
				    ipValidateObj.type = "text/javascript";
				   
				    ipValidateObj.src = "http://bus.mapbar.com:80/ip/validate.jsp?s=image" + "&" + (new Date()).getTime();
				    document.body.insertBefore(ipValidateObj, document.body.firstChild);
				  }
				}				 
